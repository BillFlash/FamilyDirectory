@model data.models.Person


<a href="/Home/AddFamily" class="btn btn-danger"><span class="glyphicon glyphicon-star"></span> Add your Family!</a>
<a href="/Home/AddPerson" class="btn btn-success"><span class="glyphicon glyphicon-heart"></span> Add a Person</a>

<body background="http://tealscientific.com/blog/wp-content/uploads/2013/01/tls20130111_DSC_3941.jpg">
    <div class=" table table-responsive">

        <table class="table-bordered">
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Picture</th>
                <th>Birthday</th>
            </tr>
            <tr>
                <td><font color="blue" size="6">@Model.FirstName</font></td>
                <td><font color="blue" size="6">@Model.LastName</font></td>
                <td><<img src="@Model.Picture" alt="Picture of family member" height="400" width="400"></td>
                <td><font color="darkred" size="6">@Model.Birthday</font></td>
            </tr>
        </table>
        <p><font color="darkblue" size="6">@Model.Bio</font></p>
    </div>
</body>

<td><a href="/Home/PersonDetail/Id"></a></td>

